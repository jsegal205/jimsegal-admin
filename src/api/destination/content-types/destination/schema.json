{
  "kind": "collectionType",
  "collectionName": "destinations",
  "info": {
    "singularName": "destination",
    "pluralName": "destinations",
    "displayName": "Destination",
    "name": "destination"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true,
    "privateAttributes": [
      "id",
      "created_at",
      "published_at",
      "updated_at"
    ]
  },
  "attributes": {
    "city": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "string"
    },
    "latitude": {
      "type": "float",
      "required": true
    },
    "longitude": {
      "type": "float"
    },
    "destination_visits": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::destination-visit.destination-visit",
      "mappedBy": "destination"
    }
  }
}
